{% set pagetitle = 'Test Authentication Sources'|trans %}
{% extends "base.twig" %}

{% block content %}
    <h1>{{ header }}</h1>
    <ul>
    {% for key, name in sources %}
        <li><a href="?as={{ name|escape('url') }}">{{ name|escape('html') }}</a></li>
    {% endfor %}
    </ul>
{% endblock %}
